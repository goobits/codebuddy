{
  "servers": [
    {
      "extensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "mjs",
        "cjs"
      ],
      "command": [
        "/workspace/node_modules/.bin/typescript-language-server",
        "--stdio"
      ]
    },
    {
      "extensions": [
        "json",
        "jsonc"
      ],
      "command": [
        "/workspace/node_modules/.bin/vscode-json-language-server",
        "--stdio"
      ]
    }
  ]
}